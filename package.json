{
  "name": "postcss-selector-namespace",
  "version": "1.2.0",
  "description": "A postcss plugin to prefix each rule with a specific selector",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "main": "dist/plugin.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublish": "babel lib --out-dir dist",
    "test": "mocha --compilers js:babel-register tests/test.js",
    "lint": "eslint lib/* tests/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topaxi/postcss-selector-namespace"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Damian Senn <damian.senn@topaxi.codes>",
  "license": "MIT",
  "keywords": [
    "postcss",
    "postcss-addon",
    "css",
    "selector",
    "prefix",
    "namespace"
  ],
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4",
    "postcss": "^5.0.14"
  }
}
